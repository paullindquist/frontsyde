\n#logo {\n\twidth: 100%;\n\tdisplay: flex;\n\tjustify-content: center;\n\timg {\n\t\tpadding-left: 2em;\n\t\tdisplay: block;\n\t\theight: 200px;\n\t}\n}\n#monitor {\n\tdisplay: flex;\n\talign-items: center;\n\tjustify-content: center;\n\tbackground: #fff;\n\theight: 300px;\n\tmax-width: 500px;\n\ttransform: perspective(135px) rotateX(2deg) rotateY(-5deg) rotateZ(8deg);\n\tborder-radius: 4px;\n\tcolor: black;\n\toverflow: hidden;\n\tmargin: 0 auto;\n}\n#stripe_0 {\n\theight: 900px;\n\twidth: 300px;\n\tbackground: $light;\n\tposition: absolute;\n\ttop: -500px;\n\tleft: -90px;\n\ttransform: rotateZ(240deg);\n}\n#stripe_1 {\n\theight: 900px;\n\twidth: 300px;\n\tbackground: $middle;\n\tposition: absolute;\n\ttop: -500px;\n\tleft: -200px;\n\ttransform: rotateZ(240deg);\n}\n.container {\n\tdisplay: grid;\n\talign-items: center;\n\tmargin: 0 auto;\n\twidth: 100%;\n  @media (min-width: 576px) {\n    min-height: 100vh;\n  }\n\tpadding: 30px;\n\tgrid-template-columns: repeat(auto-fit, minmax(360px, 1fr));\n}\n\n.welcome_message {\n\tfont-size: .7em;\n\tcolor: $dark;\n\tanimation: blink 1.6s infinite alternate;\n\ttext-shadow: 0 0 20px $dark;\n}\n.welcome,\n.showcase {\n\tpadding-bottom: 3em;\n}\n\n.company_name {\n\tfont-size: 40px;\n\tcolor: #dedede;\n}\n.learn_more {\n  display: none;\n\tposition: absolute;\n\tbottom: 0;\n\tleft: 0;\n\twidth: 100%;\n\ttext-align: center;\n\tanimation: blink 1s infinite alternate;\n\ttext-shadow: 0 0 42px $dark;\n\ttext-align: center;\n  @media (min-width: 576px) {\n\t\tdisplay: block;\n\t}\n\ta {\n\t\tdisplay: inline-block;\n\t\tcolor: $dark;\n\t\ttext-decoration: none;\n\t}\n}\n\n.down_arrow {\n\tdisplay: block;\n\tmargin: 1em auto;\n\theight: 2em;\n\tpath {\n\t\tfill: $glow;\n\t}\n\tcolor: #6ba3a5;\n\tfont-size: 12px;\n\ttext-shadow: 5px 5px 10px #2a3138;\n\ttext-shadow: 0 0 42px $dark;\n\tanimation: blink 1.5s infinite alternate;\n}\n\n#creative_container, #develop_container {\n  display: flex;\n  flex-direction: column;\n  @media (min-width: 576px) {\n    display: grid;\n  }\n  h2 {\n    order: 1;\n    line-height: 72px;\n    font-size: 72px;\n    font-weight: 700;\n    text-transform: uppercase;\n    @media (min-width: 576px) {\n      align-self: end;\n      grid-column: 1 / 2;\n      grid-row: 1;\n    }\n  }\n  .copy_container {\n    order: 3;\n    align-self: start;\n    @media (min-width: 576px) {\n      grid-row: 2;\n    }\n  }\n  #contact_image_copy {\n    display: inline;\n    float: right;\n    width: 142px;\n  }\n  #develop_image_copy {\n    display: inline;\n    float: left;\n    width: 132px;\n    margin: 10px;\n  }\n  #contact_image {\n    height: 80%;\n    width: 50%;\n    order: 2;\n    @media (min-width: 576px) {\n      display: inline-block;\n      grid-column: 2 / 3;\n      grid-row: 1 / 3;\n    }\n  }\n}\n#contact_submit {\n\twidth: 100%;\n}\n.contact_button {\n\tcolor: #2a3138;\n\tbackground: $light;\n\tborder-radius: 8px;\n\tpadding: .5em 1.5em;\n\tmargin-top: 1.5em;\n\tborder: none;\n\tfont-size: 18px;\n\t&:hover {\n\t\tbackground: $accent;\n\t\tcolor: #fff;\n\t\ttext-shadow: 1px 1px 2px #121212;\n\t}\n}\n\n#contact_container {\n\tpadding: 0;\n}\n#contact_text {\n\tpadding: 3em;\n\tbackground: #fff;\n\tcolor: #121212;\n\th2 {\n\t\tfont-size: 2.5em;\n    @media (min-width: 576px) {\n      font-size: 3em;\n    }\n\t}\n}\n#contact_us_image {\n\theight: 128px;\n\ttransform: rotateZ(8deg);\n\tfloat: right;\n\tmargin-left: 1em;\n}\n#contact_form {\n\tcolor: #fff;\n\tpadding: 3em;\n\tinput,\n\ttextarea {\n\t\tcolor: #fff;\n\t\twidth: 100%;\n\t\theight: 3em;\n\t\tmargin-bottom: 8px;\n\t\tborder-radius: 8px;\n\t\tborder: solid 1px $middle;\n\t\tbackground: #1a2229;\n\t\tpadding: 12px;\n\t}\n\ttextarea {\n\t\tpadding-top: 4px;\n\t\tpadding-left: 12px;\n\t\theight: 20em;\n\t}\n}\n\n@keyframes blink {\n\t0% {\n\t}\n\t100% {\n\t\ttext-shadow: 0 0 0px $dark;\n\t}\n}\n"},"lineCount":null}},"error":null,"hash":"08383e0fdd5e104eb8d4081e78a16c55","cacheData":{"env":{}}}